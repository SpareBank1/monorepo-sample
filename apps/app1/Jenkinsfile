pipeline {
   agent {
        docker {
            image 'docker.io/library/maven:3.6.1-jdk-8'
        }
    }

    stages {
        stage('Build app1') {
            steps {
               sh 'mvn -f apps/app1/pom.xml clean install'
//                sh 'echo "Build app1"'
            }
        }

    }
}
